{
  "name": "plan-f.de",
  "description": "The website for https://plan-f.de",
  "version": "0.1.0",
  "author": "FixMyCity <hello@fixmycity.de>",
  "license": "AGPL-3.0",
  "keywords": [],
  "private": true,
  "scripts": {
    "start": "gatsby develop --open",
    "mobile": "gatsby develop -H 0.0.0.0",
    "cs": "npm run clean && npm run start",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "build-serve": "npm run build && npm run serve",
    "clean": "gatsby clean",
    "bleach": "rm -rf node_modules/ && npm install && npm run cs",
    "check": "npm run type-check && npm run format && npm run lint && npm run test",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "updatePackages": "npx npm-check-updates -u",
    "release": "gh pr create --base main --head develop --title \"Release $(date '+%Y-%m-%d')\" --body \"\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/react-query": "^4.29.5",
    "@types/tailwindcss": "^3.0.11",
    "@types/yoga-layout": "^1.9.4",
    "clsx": "^1.2.1",
    "dotenv": "^16.0.3",
    "fs-extra": "^10.1.0",
    "gatsby": "5.8",
    "gatsby-plugin-canonical-urls": "^5.9.0",
    "gatsby-plugin-image": "^3.9",
    "gatsby-plugin-manifest": "^5.9",
    "gatsby-plugin-matomo": "^0.15.2",
    "gatsby-plugin-react-helmet": "^6.9",
    "gatsby-plugin-react-svg": "^3.3",
    "gatsby-plugin-sharp": "^5.9",
    "gatsby-plugin-sitemap": "^6.9",
    "gatsby-source-strapi": "^3.2.0",
    "gatsby-transformer-remark": "^6.9.0",
    "gatsby-transformer-sharp": "^5.9",
    "itemsjs": "^2.1.15",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.43.9",
    "tsconfig-paths-webpack-plugin": "^3.5.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@graphql-eslint/eslint-plugin": "^3.18.0",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.1",
    "@types/react": "~18.2.0",
    "@types/react-dom": "~18.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "babel-preset-gatsby": "^2.25.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-plugin-postcss": "^6.9",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
