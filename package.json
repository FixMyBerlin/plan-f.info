{
  "name": "plan-f.info",
  "description": "The website for https://plan-f.info",
  "version": "0.1.0",
  "author": "FixMyCity <hello@fixmycity.de>",
  "license": "AGPL-3.0",
  "keywords": [],
  "private": true,
  "scripts": {
    "start": "gatsby develop --open",
    "mobile": "gatsby develop -H 0.0.0.0",
    "cs": "npm run clean && npm run start",
    "build": "gatsby clean && gatsby build",
    "serve": "gatsby serve",
    "build-serve": "npm run build && npm run serve",
    "clean": "gatsby clean",
    "bleach": "rm -rf node_modules/ && npm install && npm run cs",
    "check": "npm run type-check && npm run format && npm run lint && npm run test",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "updatePackages": "npx taze major --write",
    "release": "gh pr create --base main --head develop --title \"Release $(date '+%Y-%m-%d') [skip ci]\" --body \"\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/react-query": "^4.29.25",
    "@types/yoga-layout": "^1.9.4",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "gatsby": "5.11.0",
    "gatsby-plugin-canonical-urls": "^5.11.0",
    "gatsby-plugin-image": "^3.11.0",
    "gatsby-plugin-manifest": "^5.11.0",
    "gatsby-plugin-matomo": "^0.16.0",
    "gatsby-plugin-react-svg": "^3.3",
    "gatsby-plugin-sharp": "^5.11.0",
    "gatsby-plugin-sitemap": "^6.11.0",
    "gatsby-source-strapi": "^3.3.0",
    "gatsby-transformer-remark": "^6.11.0",
    "gatsby-transformer-sharp": "^5.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@tailwindcss/forms": "^0.5.4",
    "@testing-library/dom": "^8.20.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@types/react": "~18.2.15",
    "@types/react-dom": "~18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.6.1",
    "babel-preset-gatsby": "^3.11.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-plugin-postcss": "^6.11.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "postcss": "^8.4.26",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
