{
  "name": "plan-f.de",
  "description": "The website for https://plan-f.de",
  "version": "0.1.0",
  "author": "FixMyCity <hello@fixmycity.de>",
  "license": "AGPL-3.0",
  "keywords": [],
  "private": true,
  "scripts": {
    "start": "gatsby develop --open",
    "mobile": "gatsby develop -H 0.0.0.0",
    "cs": "npm run clean && npm run start",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "build-serve": "npm run build && npm run serve",
    "clean": "gatsby clean",
    "bleach": "rm -rf node_modules/ && npm install && npm run cs",
    "check": "npm run type-check && npm run lint && npm run format && npm run test",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^2.0.11",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "@tanstack/react-query": "^4.0.10",
    "@types/tailwindcss": "^3.0.10",
    "classnames": "^2.3.1",
    "embla-carousel-react": "^7.1.0",
    "fs-extra": "^10.1.0",
    "gatsby": "5.8",
    "gatsby-plugin-canonical-urls": "^5.9.0",
    "gatsby-plugin-image": "^3.9",
    "gatsby-plugin-layout": "^4.9",
    "gatsby-plugin-manifest": "^5.9",
    "gatsby-plugin-react-helmet": "^6.9",
    "gatsby-plugin-react-svg": "^3.3",
    "gatsby-plugin-sharp": "^5.9",
    "gatsby-plugin-sitemap": "^6.9",
    "gatsby-source-strapi": "^3.2.0",
    "gatsby-transformer-remark": "^6.9.0",
    "gatsby-transformer-sharp": "^5.9",
    "itemsjs": "^2.1.15",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.34.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@graphql-eslint/eslint-plugin": "^3.10.7",
    "@tailwindcss/forms": "^0.5.2",
    "@types/jest": "^28.1.1",
    "@types/node": "^18.0.0",
    "@types/react": "~17.0.37",
    "@types/react-dom": "~17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^28.1.1",
    "babel-preset-gatsby": "^2.3.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-plugin-postcss": "^6.9",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
